<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp"
    tools:context="hdd.com.meikuang.ZhuActivity">

    <LinearLayout
        android:layout_marginTop="120dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:layout_margin="5dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="intenfengji"
            android:background="@drawable/fengji"/>
        <ImageView
            android:layout_width="0dp"
            android:layout_height="104dp"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:onClick="intertidal"
            android:background="@drawable/pidai"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="0dp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="inten"
            android:background="@drawable/paishui"/>
        <ImageView
            android:layout_width="0dp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="intengongre"
            android:background="@drawable/gongre"/>
    </LinearLayout>
</LinearLayout>
