<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hdd.com.meikuang.Fragment.Fragment3">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        android:background="@drawable/shuiwei">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="110dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/id_s11"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center" />

            <TextView
                android:id="@+id/id_s12"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>

            <TextView
                android:id="@+id/id_s13"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>

            <TextView
                android:id="@+id/id_s14"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/id_s21"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"/>

            <TextView
                android:id="@+id/id_s22"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>

            <TextView
                android:id="@+id/id_s23"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>

            <TextView
                android:id="@+id/id_s24"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:gravity="center"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
